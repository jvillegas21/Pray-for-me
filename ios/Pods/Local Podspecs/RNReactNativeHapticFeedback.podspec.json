{
  "name": "RNReactNativeHapticFeedback",
  "version": "2.3.3",
  "summary": "Basic haptic feedback for iOS and android",
  "description": "react-native-haptic-feedback",
  "homepage": "https://github.com/mkuczera/react-native-haptic-feedback",
  "license": "MIT",
  "authors": {
    "author": "m.kuczera@gmail.com"
  },
  "platforms": {
    "ios": "12.4"
  },
  "source": {
    "git": "https://github.com/mkuczera/react-native-haptic-feedback.git",
    "tag": "v2.3.3"
  },
  "source_files": "ios/**/*.{h,m,mm}",
  "requires_arc": true,
  "compiler_flags": "-DFOLLY_NO_CONFIG -DFOLLY_MOBILE=1 -DFOLLY_USE_LIBCPP=1 -Wno-comma -Wno-shorten-64-to-32",
  "dependencies": {
    "React-Core": [],
    "RCT-Folly": [
      "2021.07.22.00"
    ]
  },
  "pod_target_xcconfig": {
    "HEADER_SEARCH_PATHS": "\"$(PODS_ROOT)/boost\"",
    "CLANG_CXX_LANGUAGE_STANDARD": "c++17"
  }
}
